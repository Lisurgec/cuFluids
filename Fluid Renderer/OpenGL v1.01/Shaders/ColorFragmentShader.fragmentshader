#version 330 core

// Interpolated values from the vertex shaders
in vec2 UV;

// Ouput data
//out vec4 color;

void main(){

	// Output color = color specified in the vertex shader, 
	// interpolated between all 3 surrounding vertices
	gl_FragColor = vec4(1.0 * UV, 1.0, 1.0,1.0);
	//gl_FragColor = vec4(50.0 / 256.0, 120.0 / 256.0 , 120.0 / 256.0, 0.2);
}
